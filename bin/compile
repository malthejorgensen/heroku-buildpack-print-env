#!/bin/sh

BP_DIR=$(cd $(dirname $0); cd ..; pwd)

indent() {
  sed -u 's/^/       /'
}

echo '-----> Found a "export"-file'

echo 'Printing contents of "export"-file: '
cat $BP_DIR/export | indent

echo 'Print environment'
env | indent

ls /app/.apt
ls /app/.apt/usr
ls /app/.apt/usr/lib
ls /app/.apt/usr/include
ls /app/.apt/usr/include/xmlsec1
